 <!--Gear retraction-->
 <animation>
   <type>rotate</type>
   <object-name>wheel</object-name>
   <object-name>gear1</object-name>
   <object-name>gear2</object-name>
   <object-name>gear4</object-name>
   <property>gear/gear/position-norm</property>
   <interpolation>
     <entry>
       <ind>1.0</ind>
       <dep>0</dep>
     </entry>
     <entry>
       <ind>0.1</ind>
       <dep>-125</dep>
     </entry>
     <entry>
       <ind>0</ind>
       <dep>-125</dep>
     </entry>
      </interpolation>
		<center>
			<x-m>-1.52345</x-m>
			<y-m>0.0</y-m>
			<z-m>-0.97144</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
 </animation>
 <animation>
   <type>rotate</type>
   <object-name>gear3</object-name>
   <property>gear/gear/position-norm</property>
   <interpolation>
     <entry>
       <ind>1.0</ind>
       <dep>0</dep>
     </entry>
     <entry>
       <ind>0.1</ind>
       <dep>70</dep>
     </entry>
     <entry>
       <ind>0</ind>
       <dep>70</dep>
     </entry>
      </interpolation>
		<center>
			<x-m>-1.26217</x-m>
			<y-m>0.0</y-m>
			<z-m>-0.96166</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
 </animation>
 <!--Gear Compression-->
 <animation>
   <type>translate</type>
   <object-name>gear1</object-name>
   <object-name>gear2</object-name>
   <object-name>gear3</object-name>
   <object-name>gear4</object-name>
   <object-name>wheel</object-name>
   <property>gear/gear[1]/compression-m</property>
   <factor>1</factor>
    <axis>
            <x>0.5</x>
            <y>0</y>
            <z>1</z>
    </axis>
 </animation>
 <!--Wheel spin-->
 <animation>
   <type>spin</type>
   <object-name>wheel</object-name>
   <property>gear/gear[1]/rollspeed-ms</property>
   <factor>50</factor>
    <center>
            <x-m>-1.56868</x-m>
            <y-m>0.0</y-m>
            <z-m>-1.33311</z-m>
    </center>
    <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
    </axis>
 </animation>
 
